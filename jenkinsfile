pipeline {
	agent any
	tools {
		maven 'M2_HOME'
}
	stages {
		stage{"Build"} {
			step {
			sh 'mvn clean'
			sh 'mvn install'
			sh 'mvn package'

	}

}

	stage{'Test'} {
		steps {
			sh 'mvn test'
	}

}

	stages{'deploy'} {
		steps {
			echo 'deploy step'
			sleep 4
	}

}

	stage{'Docker'} {
		steps {
			echo 'image step'

			}
		}
	}
}
